{
    "name": "SteamCode Chrome Extension", 
    "description": "Sample Chrome Extension", 
    "version": "1.0",
    "manifest_version": 3, 
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab"],
    "action": {
        "default_popup": "popup.html"
    }, 
    "content_scripts": [
        {
            "matches": ["https://*.google.com/*"],
            "js": ["content.js"]
        }
    ]
}